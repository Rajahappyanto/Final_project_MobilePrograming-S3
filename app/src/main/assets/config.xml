<?xml version="1.0" encoding="utf-8"?>

<data>

	<settings>

		<assets>

			<packages default="assets" audio="assets.audio" />

		</assets>

		<font name="roboto-webfont" />

		<asciiArt></asciiArt>

		<urls>

			<website>https://institutjacob.com</website>

			<author>Gradle Code</author>

		</urls>

		<audioHoldDelay>5000</audioHoldDelay>

		<disableEyeCandy>false</disableEyeCandy>

		<disableDeviceMotion>false</disableDeviceMotion>

