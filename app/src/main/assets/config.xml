		<rank>

			<good>Winner!</good>

			<ok  >Podium finish!</ok>

			<poor>Race completed!</poor>

		</rank>

		<scenes>

			<instructions>

				<title>How to play</title>

				<message><![CDATA[Compete across the world as you race in the Car Driver.<br/><br/>Dodge opponents, boost around twisting corners, collect coins and upgrade your car for the next race.<br/><br/>Are you the best Car Driver ?]]></message>

			</instructions>

			<selectLevel>

				<series>Championship __X__, </series>

				<title>Race __X__ of 4</title>

			</selectLevel>

			<interstitial>

				<total>Total points</total>

				<personalBest>Personal best</personalBest>

			</interstitial>

			<results>

				<title>Race __X__ of 4 complete</title>

				<table>

					<location>Race</location>

					<position>Position</position>

					<points>Points</points>

					<total>Total points</total>

					<personalBest>Personal best</personalBest>

				</table>

				<message>You finished the race in __SCORE__ position and scored __POINTS__ points.</message>

			</results>

			<shop>

				<title>Upgrade your car</title>

				<maxed>Maxed</maxed>

				<insufficient>Earn Funds</insufficient>

			</shop>

