			<finished>Results</finished>

			<reset>Reset game</reset>

			<buy>Buy</buy>

			<countdown>Auto start</countdown>

			<unpause>Resume</unpause>

			<audioOn>Sound On</audioOn>

			<audioOff>Sound Off</audioOff>

			<back>Quit game</back>

			<testMode>

				<finish>Finish</finish>

				<skip>Skip</skip>

			</testMode>

		</buttons>

		<locations>

			<a alt="Dunes" difficulty="Easy">Desert</a>

			<b alt="Trail" difficulty="Medium">Forest</b>

			<c alt="Tundra" difficulty="Hard">Mountain</c>

			<d alt="Tour" difficulty="Expert">Lakeside</d>

		</locations>

		<upgrades>

			<speed>Top speed</speed>

			<steering>Grip</steering>

			<acceleration>Acceleration</acceleration>

